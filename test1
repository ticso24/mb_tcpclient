while true ; do
	sleep 1
	./mb_tcpclient c13 1000 2  write_coil 0 1 &
	./mb_tcpclient c13 1000 2  write_coil 1 1 &
	./mb_tcpclient c13 1000 2  write_coil 2 1 &
	./mb_tcpclient c13 1000 2  write_coil 3 1 &
	./mb_tcpclient c13 1000 2  write_coil 4 1 &
	./mb_tcpclient c13 1000 2  write_coil 5 1 &
	./mb_tcpclient c13 1000 2  write_coil 6 1 &
	./mb_tcpclient c13 1000 2  write_coil 7 1 &
	./mb_tcpclient c13 1000 2  write_coil 8 1 &
	./mb_tcpclient c13 1000 2  write_coil 9 1 &
	./mb_tcpclient c13 1000 2  write_coil 10 1 &
	./mb_tcpclient c13 1000 2  write_coil 11 1 &
	sleep 1
	./mb_tcpclient c13 1000 2  write_coil 0 0 &
	./mb_tcpclient c13 1000 2  write_coil 1 0 &
	./mb_tcpclient c13 1000 2  write_coil 2 0 &
	./mb_tcpclient c13 1000 2  write_coil 3 0 &
	./mb_tcpclient c13 1000 2  write_coil 4 0 &
	./mb_tcpclient c13 1000 2  write_coil 5 0 &
	./mb_tcpclient c13 1000 2  write_coil 6 0 &
	./mb_tcpclient c13 1000 2  write_coil 7 0 &
	./mb_tcpclient c13 1000 2  write_coil 8 0 &
	./mb_tcpclient c13 1000 2  write_coil 9 0 &
	./mb_tcpclient c13 1000 2  write_coil 10 0 &
	./mb_tcpclient c13 1000 2  write_coil 11 0 &
