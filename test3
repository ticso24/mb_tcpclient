#mb_tcpbridge /dev/ubmb1 0.0.0.0 1000
mb_tcpclient c13 1000 2 write_coil 1 1 &
mb_tcpclient c13 1000 2 write_coil 3 1 &
mb_tcpclient c13 1000 2 write_coil 5 1 &
sleep 1
mb_tcpclient c13 1000 2 write_coil 1 0 &
mb_tcpclient c13 1000 2 write_coil 3 0 &
mb_tcpclient c13 1000 2 write_coil 5 0 &
sleep 1

